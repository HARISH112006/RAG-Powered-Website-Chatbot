# RAG Chatbot Frontend Configuration

# Backend API URL - Replace with your ngrok URL or production URL
VITE_API_BASE_URL=http://localhost:8000

# For ngrok deployment, use your ngrok URL:
# VITE_API_BASE_URL=https://your-ngrok-id.ngrok.io

# For production deployment:
# VITE_API_BASE_URL=https://your-production-domain.com

# App Configuration
VITE_APP_NAME=RAG Intelligence
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MULTI_LANGUAGE=true
VITE_ENABLE_DEBUG_MODE=true